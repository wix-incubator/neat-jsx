{
	"name": "neat-jsx",
	"displayName": "neat-jsx",
	"description": "jsx cleaner",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.neatjsx"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.neatjsx",
				"title": "Neat Jsx"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.4.3",
		"@types/react": "^16.8.13",
		"react": "^16.8.6",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"@babel/core": "^7.4.3",
		"@babel/parser": "^7.4.3",
		"@babel/traverse": "^7.4.3",
		"@babel/types": "^7.4.0",
		"@types/babel__core": "^7.1.1",
		"@types/babel__traverse": "^7.0.6",
		"@types/glob": "^7.1.1",
		"@types/mkdirp": "^0.5.2",
		"gitignore-to-glob": "^0.3.0",
		"glob": "^7.1.3",
		"mkdirp": "^0.5.1",
		"relative": "^3.0.2"
	}
}
